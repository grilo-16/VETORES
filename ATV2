#include <stdio.h>

int main() {
    int valores[6];

    // Solicitar ao usu√°rio que insira os 6 valores
    printf("Digite 6 valores inteiros:\n");

    for (int i = 0; i < 6; i++) {
        printf("Valor %d: ", i + 1);
        scanf("%d", &valores[i]);
    }

    // Exibir os valores lidos na tela
    printf("Valores lidos:\n");

    for (int i = 0; i < 6; i++) {
        printf("%d\n", valores[i]);
    }

    return 0;
}
